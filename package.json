{
  "name": "nodde-backend",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev:server": "cross-env NODE_ENV=development sls offline start --stage offline",
    "dev": "run-p dev:*",
    "tail-log": "sls logs -f app -t",
    "deploy:app": "cross-env NODE_ENV=production sls deploy --stage prod",
    "deploy-prod": "run-s deploy:*",
    "remove-prod": "cross-env NODE_ENV=production sls remove --stage prod",
    "clean": "rimraf dist .esbuild .serverless",
    "lint": "eslint .",
    "format": "eslint --fix . && prettier '**/*.{json,yaml}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "test:dev": "start-server-and-test dev http://localhost:4000 'curl --head http://localhost:4000'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.321.1",
    "@aws-sdk/client-lambda": "^3.322.0",
    "@aws-sdk/client-s3": "^3.321.1",
    "@aws-sdk/lib-dynamodb": "^3.327.0",
    "@aws-sdk/types": "^3.310.0",
    "@aws-sdk/util-dynamodb": "^3.321.1",
    "@types/uuid": "^9.0.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint-config-airbnb": "^19.0.4",
    "ethers": "5.6.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "md5": "^2.3.0",
    "serverless-http": "^3.2.0",
    "siwe": "^2.1.4",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/md5": "^2.3.2",
    "@types/node": "^18.16.2",
    "concurrently": "^8.0.1",
    "esbuild": "^0.17.18",
    "nodemon": "^2.0.22",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.43.1",
    "serverless-offline": "^12.0.4",
    "serverless-plugin-typescript": "^2.1.4",
    "typescript": "^5.0.4"
  }
